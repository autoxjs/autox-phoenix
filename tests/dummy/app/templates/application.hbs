<div class="jumbotron">
  <div class="container">
    <div class="row">
      <div class="col-xs-12">
        <h2 id="title">Welcome to Ember</h2>
        {{#if session.loggedIn}}
          <small>You are logged in!</small>
        {{/if}}
      </div>
    </div>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-xs-12">
      <div class="btn-group">
        {{#link-to "chairs.index" class="btn btn-default"}}chairs index{{/link-to}}
        {{#link-to "chairs.new" class="btn btn-default"}}new chair{{/link-to}}
        {{#link-to "chair.index" 1 class="btn btn-default"}}chair 1{{/link-to}}
        {{#link-to "chair.edit" 1 class="btn btn-default"}}edit chair 1{{/link-to}}
      </div>
      
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      {{outlet}}
    </div>
  </div>
</div>
