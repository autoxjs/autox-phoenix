{{#if userHasDefinedComponent}}
  {{component userDefinedComponent model=model}}
{{else}}
  {{#each fields as |field|}}
    {{#if field.contDisplay}}
      <div class="autox-show-for__list-member list-group-item">
        <strong class="autox-show-for__label list-group-item-heading">{{field.label}}</strong>
        <div class="autox-show-for__content list-group-item-text">
          {{#if field.isRelationship}}
            {{get model (concat field.name "." field.proxyKey)}}
          {{else}}
            {{txt (get model field.name) type=field.type formatter=field.presenter}}
          {{/if}}
        </div>
        <small class="autox-show-for__description">{{field.description}}</small>
      </div>
    {{/if}}
  {{else}}
    <div class="autox-show-for__list-member list-group-item">
      <strong class="autox-show-for__label list-group-item-heading">No Fields Are Avaible to Show!</strong>
      <small class="autox-show-for__description">You probably did something wrong</small>
    </div>
  {{/each}}
{{/if}}